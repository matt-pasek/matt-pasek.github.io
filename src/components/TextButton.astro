---
export interface props {
    text: string;
    href: string;
}

import '@fontsource/oxygen/400.css';

const { text, href } = Astro.props;
---

<a href={href}><button class="font-oxygen text-xs bg-accent-light text-light dark:text-dark dark:bg-accent-dark px-5 py-3 rounded-xl">{text}</button></a>